module generating/pixiedust/execute

imports
  api/module-names-api

  lib/scopes
  
  generating/pixiedust/expression

rules

  modulename-to-execute-stmts = (id, modulename-get-exprs) ; env-fold-mapconcat(exp-to-js-stmts ; (id, output-exp-stmts))
    
  output-exp-stmts : (exp_e, stmts_e) -> js-stmt* |[
    stmts_e
    console.log(exp_e);
  ]|