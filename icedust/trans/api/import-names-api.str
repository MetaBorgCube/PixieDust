module api/import-names-api

imports
  lib/nabl2

rules
  importpath-get-imports = get-property(|"imports")
  
rules //function imports
  functionname-is-imported = where(functionname-get-importpath)
  
  functionname-get-importpath = get-decl(|"Function") ; get-property(|"Function", "importedFrom")
  functionname-get-importname = get-decl(|"Function") ; get-property(|"Function", "importName")