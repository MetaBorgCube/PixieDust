module api/expressions-api

imports // signatures

  signatures/Expressions-sig
  signatures/Types-sig  

imports // functions

  api/types-api

rules

  is-expr = not(is-string);not(is-type) // identifiers inside expressions are not expressions, neither are type constructors
  
  is-literal = ?Int(_)
  is-literal = ?Float(_)
  is-literal = ?True()
  is-literal = ?False()
  is-literal = ?Datetime(_)
  is-literal = ?NoValue()
  is-literal = ?LitString(_)
  
  literal-multiplicity = ?This() ; !One()
  literal-multiplicity = ?NoValue() ; !ZeroOrOne()
  literal-multiplicity = is-literal ; !One()
  
  