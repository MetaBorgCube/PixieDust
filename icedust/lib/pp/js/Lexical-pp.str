module pp/js/Lexical-pp

imports
  libstratego-gpp
  libspoofax/sdf/pp
  libspoofax/term/origin
  libstratego-sglr
  signatures/js/Lexical-sig


imports
  signatures/js/Common-sig


imports
  pp/js/Common-pp


strategies
  prettyprint-example =
    prettyprint-javascript

  prettyprint-javascript =
    fail

  prettyprint-javascript(|sort) =
    fail


strategies
  prettyprint-SDF-start-symbols =
    prettyprint-javascript-start-symbols

  prettyprint-javascript-start-symbols =
    fail


strategies
  prettyprint-javascript =
    prettyprint-javascript-KEYWORD

  prettyprint-javascript(|sort):
    t -> <prettyprint-javascript-KEYWORD>
    where <?"KEYWORD"> sort

  prettyprint-javascript-KEYWORD =
    ![S(<is-string>)]

  prettyprint-javascript-KEYWORD :
    amb([h|hs]) -> <prettyprint-javascript-KEYWORD> h